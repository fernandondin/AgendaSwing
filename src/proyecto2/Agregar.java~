    private void guardarMouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_guardarMouseClicked
        if(!validaCorreo(correo.getText())){
            JOptionPane.showMessageDialog(this,"Ingrese un correo valido","Correo invalido",
                    JOptionPane.ERROR_MESSAGE);
            return;
        }
        if(casa.getText().length()<8 || casa.getText().length()>8){
            JOptionPane.showMessageDialog(this,"Ingrese un numero valido","Numero invalido",
                    JOptionPane.ERROR_MESSAGE);
            return;
        }
        if(celular.getText().length()<10 || celular.getText().length()>10){
            JOptionPane.showMessageDialog(this,"Ingrese un numero de celular valido","Numero invalido",
                    JOptionPane.ERROR_MESSAGE);
            return;
        }
        Datos datos = new Datos();
        Contacto nuevo = new Contacto(nombre.getText(),correo.getText(),casa.getText(),celular.getText());
        if(editar==null){
        arbol.add(nuevo);
        try {
            datos.escribe(arbol,"arbol");
        } catch (ClassNotFoundException | IOException ex) {
            Logger.getLogger(AgendaInterfaz.class.getName()).log(Level.SEVERE, null, ex);
        }
        ai.actualizaLista();
        }else{
        Contacto nuevo1 = new Contacto(nombre.getText(),correo.getText(),casa.getText(),celular.getText());
        int a=1;
        if(arbol.contains(nuevo1)){
            a=JOptionPane.showConfirmDialog(this,"Ya hay un contacto identico existente, ¿Continuar?");
            if(a==1)
                return;
            arbol.remove(editar);   
        }
        arbol.remove(editar);
        arbol.add(nuevo1);
        try {
            datos.escribe(arbol,"arbol");
        } catch (ClassNotFoundException | IOException ex) {
            Logger.getLogger(AgendaInterfaz.class.getName()).log(Level.SEVERE, null, ex);
        }
        ai.actualizaLista();
        }
        this.dispose();
    }//GEN-LAST:event_guardarMouseClicked
    private  boolean validaCorreo(String correo) {
       String PATTERN_EMAIL = "^[_A-Za-z0-9-\\+]+(\\.[_A-Za-z0-9-]+)*@"+ 
               "[A-Za-z0-9-]+(\\.[A-Za-z0-9]+)*(\\.[A-Za-z]{2,})$";
        Pattern pattern = Pattern.compile(PATTERN_EMAIL);
        Matcher matcher = pattern.matcher(correo);
        return matcher.matches();
 
    }
    private void casaKeyTyped(java.awt.event.KeyEvent evt) {//GEN-FIRST:event_casaKeyTyped
        char c = evt.getKeyChar();
        if((c<'0' || c>'9') || casa.getText().length()>7){
           evt.consume();
        }
        validaTexto(evt);
    }//GEN-LAST:event_casaKeyTyped

    private void celularKeyTyped(java.awt.event.KeyEvent evt) {//GEN-FIRST:event_celularKeyTyped
        char c = evt.getKeyChar();
        if(c<'0' || c>'9' || celular.getText().length()>9)
            evt.consume();
        validaTexto(evt);
    }//GEN-LAST:event_celularKeyTyped

    private void nombreKeyTyped(java.awt.event.KeyEvent evt) {//GEN-FIRST:event_nombreKeyTyped
        if(nombre.getText().equals("Nombre")){
            nombre.setText("");
        }
        char c = evt.getKeyChar();
        if(!(c<'0' || c>'9'))
            evt.consume();
        validaTexto(evt);
    }//GEN-LAST:event_nombreKeyTyped

    private void nombreFocusGained(java.awt.event.FocusEvent evt) {//GEN-FIRST:event_nombreFocusGained
        
    }//GEN-LAST:event_nombreFocusGained

    private void casaFocusGained(java.awt.event.FocusEvent evt) {//GEN-FIRST:event_casaFocusGained
        if(casa.getText().equals("Número de casa")){
            casa.setText("");
        }
    }//GEN-LAST:event_casaFocusGained

    private void casaFocusLost(java.awt.event.FocusEvent evt) {//GEN-FIRST:event_casaFocusLost
       if(casa.getText().equals("")){
            casa.setText("Número de casa");
        }
    }//GEN-LAST:event_casaFocusLost

