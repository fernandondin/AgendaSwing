    public Agregar(TreeSet<Contacto> arbol,AgendaInterfaz ai) {
        initComponents();
        this.setLocationRelativeTo(null);
        this.setResizable(false);
        this.ai=ai;
        guardar.setToolTipText("Guardar");
        cancelar.setToolTipText("Cancelar");
        jLabel3.setBounds(-320,jLabel3.getY(),jLabel3.getWidth(),jLabel3.getHeight());
        nombre.setBounds(-10,nombre.getY(),nombre.getWidth(),nombre.getHeight());
        casa.setBounds(-10,casa.getY(),casa.getWidth(),casa.getHeight());
        celular.setBounds(-10,celular.getY(),celular.getWidth(),celular.getHeight());
        correo.setBounds(-10,correo.getY(),correo.getWidth(),correo.getHeight());
        animacion();
        this.setDefaultCloseOperation(JFrame.DISPOSE_ON_CLOSE);
        this.arbol=arbol;
    }
    public Agregar(TreeSet<Contacto> arbol,AgendaInterfaz ai,Contacto editar){
        initComponents();
        this.setLocationRelativeTo(null);
        this.setResizable(false);
        this.ai=ai;
        guardar.setToolTipText("Guardar");
        cancelar.setToolTipText("Cancelar");
        jLabel3.setBounds(-320,jLabel3.getY(),jLabel3.getWidth(),jLabel3.getHeight());
        nombre.setBounds(-10,nombre.getY(),nombre.getWidth(),nombre.getHeight());
        casa.setBounds(-10,casa.getY(),casa.getWidth(),casa.getHeight());
        celular.setBounds(-10,celular.getY(),celular.getWidth(),celular.getHeight());
        correo.setBounds(-10,correo.getY(),correo.getWidth(),correo.getHeight());
        animacion();
        this.setDefaultCloseOperation(JFrame.DISPOSE_ON_CLOSE);
        this.arbol=arbol;
        this.editar=editar;
        nombre.setText(this.editar.getNombre());
        casa.setText(this.editar.getCasa());
        celular.setText(this.editar.getCelular());
        correo.setText(this.editar.getCorreo());
        jLabel3.setText("Editar Contacto");
        this.setTitle("Editar");
    }
    private void animacion(){
        Animacion a = new Animacion(jLabel3,-320,jLabel3.getY(),0,Animacion.ANIMA_XR,2);
        a.anima();
        Animacion a1 = new Animacion(nombre,-10,nombre.getY(),75,Animacion.ANIMA_XR,7);
        a1.anima();
        Animacion a2 = new Animacion(casa,-10,casa.getY(),65,Animacion.ANIMA_XR,9);
        a2.anima();
        Animacion a3 = new Animacion(celular,-10,celular.getY(),65,Animacion.ANIMA_XR,11);
        a3.anima();
        Animacion a4 = new Animacion(correo,-10,correo.getY(),65,Animacion.ANIMA_XR,13);
        a4.anima();
    }
